  <script type="text/javascript"
    src="https://appleid.cdn-apple.com/appleauth/static/jsapi/appleid/1/en_US/appleid.auth.js"></script>
</head>

<body>
  <% if ((BUILD_ENV==='production' || DEBUG_TOOLS) && APPS.GOOGLE_TAG_MANAGER) { %>
